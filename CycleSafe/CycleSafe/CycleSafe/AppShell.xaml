<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:CycleSafe.Views"
             x:Class="CycleSafe.AppShell">
    <FlyoutItem Title="Connect Page"
                Icon="bluetooth.png">
        <ShellContent ContentTemplate="{DataTemplate local:ConnectPage}"/>       
    </FlyoutItem>
    <FlyoutItem Title="Home Screen">
        <ShellContent ContentTemplate="{DataTemplate local:HomeScreen}"/>
    </FlyoutItem>
</Shell>